<!--面授班测验模块-->
<view class="contain hb-box">
    <view class="hb-box-bd">
        <view class="view-pos">
            <view class="course-detail-hd">
                <view class="course-detail-img">
                    <image class="img"  mode="widthFix" src="/images/course-img.jpg" alt=" " />
                </view>
                <view class="btn-primary">查看考核</view>
                <view class="status-label-box ing">进行中</view>
                <!--<view class="seal seal-1"><image class="img" src="/images/seal-pass.png" alt=" " /></view>-->
                <view class="course-info">
                    <image class="mask" src="/images/course-img-mask.png" alt=" " />
                    <view class="course-name-1">
                        <image class="img" src="/images/icon-address.png" alt=" " />
                        <text class="txt">当前位置：海峡技术转移中心海峡技术转移中心海峡技术转移中心</text>
                        <!--<view class="btn">获取授权</view>-->
                    </view>
                </view>
            </view>
            <view class="course-detail-tab course-detail-tab-1">
                <view class="tab-items">
                    <view class="ico">
                        <image class="img" src="/images/course-tab-course.png" alt=" " />
                        <!--<image class="img" src="/images/course-tab-course-current.png" alt=" " />-->
                    </view>
                    <view class="txt">
                        <view>课程目录</view>
                    </view>
                    <image class="current-bg" src="/images/tab-current-bg.jpg" alt=" " />
                </view>
                <view class="tab-items current">
                    <view class="ico">
                        <!--<image class="img" src="/images/course-tab-exam.png" alt=" " />-->
                        <image class="img" src="/images/course-tab-exam-current.png" alt=" " />
                    </view>
                    <view class="txt">
                        <view>测验模块</view>
                    </view>
                    <image class="current-bg" src="/images/tab-current-bg.jpg" alt=" " />
                </view>
                <view class="tab-items">
                    <view class="ico">
                        <image class="img" src="/images/course-tab-questionnaire.png" alt=" " />
                        <!--<image class="img" src="/images/course-tab-questionnaire-current.png" alt=" " />-->
                    </view>
                    <view class="txt">
                        <view>问卷调查</view>
                    </view>
                    <image class="current-bg" src="/images/tab-current-bg.jpg" alt=" " />
                </view>
            </view>
        </view>
        <view class="course-detail-tab-bd course-detail-tab-bd-1">
            <view class="block-box block-box-tit fs30">线上考试</view>
            <view class="course-detail-exam-info block-box">
                <view class="fs30 to">我是这次考试试卷的名称名称名称</view>
                <view class="info-bd">
                    <view>考试总分：100分</view>
                    <view>考试达标要求：成绩>=60分</view>
                    <view>剩余考试次数：<text class="ci">1</text>次/<text class="ci">不限</text></view>
                    <view>考试时长：<text class="ci">60</text>分钟</view>
                    <view>测验合格分数：<text class="ci">60</text>分</view>
                </view>
                <view class="hb-warm-tips hb-warm-tips-2 mt20">
                    <image class="ico-tips" src="/images/class-detail-warm.png" />
                    <view class="tips-txt">
                        <view>开放考试时间：</view>
                        <view>2018-05-21  12:12  至  2018-05-21  13:12</view>
                    </view>
                </view>
                <view class="seal"><image class="img" src="/images/seal-exam-g.png" alt=" " /></view>
            </view>
            <view class="block-box bor-b20">
                <view class="exam-enter-box">
                    <view class="exam-enter">
                        <view class="ico">
                            <image class="img" src="/images/icon-exam-enter.png" alt=" " />
                        </view>
                        <view class="mt30">进入测验</view>
                    </view>
                    <view class="exam-enter">
                        <view class="ico">
                            <image class="img" src="/images/icon-exam-history.png" alt=" " />
                        </view>
                        <view>历史测验记录</view>
                    </view>
                </view>
            </view>

            <view class="block-box block-box-tit fs30">线下考试</view>
            <view class="course-detail-exam-info block-box bor-b20">
                <view class="fs30 to">我是这次考试试卷的名称名称名称</view>
                <view class="situation-info-box">
                    <view class="info-desc info-desc-2"><view class="lt">考试总分：</view><view class="rt">100分</view></view>
                    <view class="info-desc info-desc-2"><view class="lt">考试地点：</view><view class="rt">福建省福州市工业路611号福建省福州市工业路611号福建省福州市工业路611号</view></view>
                    <view class="info-desc info-desc-2"><view class="lt">考试成绩：</view><view class="rt ci">暂未开始考试</view></view>
                </view>

                <view class="hb-warm-tips hb-warm-tips-3 mt20">
                    <view class="fs28 cr">由于考试是线下开展的，请提前10~30分钟至考试地点，准备考试！</view>
                    <view class="flex mt10">
                        <image class="ico-tips" src="/images/class-detail-warm.png" />
                        <view class="tips-txt">
                            <view>开放考试时间：</view>
                            <view>2018-05-21  12:12  至  2018-05-21  13:12</view>
                        </view>
                    </view>
                </view>
                <view class="seal"><image class="img" src="/images/seal-exam-g.png" alt=" " /></view>
            </view>

            <view class="block-box block-box-tit fs30">练习</view>
            <view class="block-box">
                <view class="exam-enter-box">
                    <view class="exam-enter">
                        <view class="ico">
                            <image class="img" src="/images/icon-test-enter.png" alt=" " />
                        </view>
                        <view>做练习</view>
                    </view>
                    <view class="exam-enter">
                        <view class="ico">
                            <image class="img" src="/images/icon-test-history.png" alt=" " />
                        </view>
                        <view>历史练习试题</view>
                    </view>
                </view>
            </view>
        </view>
    </view>
    <view class="hb-tabbar hb-tabbar-4">
        <view class="tabbar-items">
            <image class="tabbar-icon" src="/images/tabbar-contact.png" alt=" " />
            <text>通讯录</text>
        </view>
        <view class="tabbar-items">
            <image class="tabbar-icon" src="/images/tabbar-notice.png" alt=" " />
            <text>通知</text>
        </view>
        <view class="tabbar-items">
            <image class="tabbar-icon" src="/images/tabbar-photo.png" alt=" " />
            <text>班级风采</text>
        </view>
        <view class="tabbar-items">
            <image class="tabbar-icon" src="/images/tabbar-lodging.png" alt=" " />
            <text>住宿信息</text>
        </view>
    </view>
</view>
